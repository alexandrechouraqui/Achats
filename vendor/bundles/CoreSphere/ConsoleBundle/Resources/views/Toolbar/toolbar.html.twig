<div id="coresphere-console" style="display: none; top:20px; position: absolute; width: 100%;"></div>

<span id="coresphere-console-click" style="display:block; position: fixed; bottom:10px; right:50px; z-index: 6000001;">
	<img
    	alt="Show console"
        style="cursor: pointer; width: 20px"
        src="data:image/png;base64,"
	/>

	<div id="coresphere-console" style="display:none; position:absolute;right:0px;width:80%;"></div>
</span>

<script type="text/javascript">
	window.jQuery || document.write('<script src="{{ asset('bundles/coresphereconsole/js/jquery-1.6.2.min.js') }}"><\/script>');
</script>

<script type="text/javascript">
	jQuery(function () {
    	$('#coresphere-console-click').click(function() {
        	$console = $('#coresphere-console');

			if ($console.is(':visible')) {
				$console.hide();
			} else {
    			if (!$console.html()) {
    				$console.load("{{ path('console', {'raw': true}) }}");
    			}
    			$console.show();
			}
		});
	});
</script>